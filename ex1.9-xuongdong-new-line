 # \n xuong dong moi
 # Phan biet khi co n va khong co \n

producer = "Samsung Lenovo Huawei LG Iphone"
product = "\n Note10 \n Fablet\n Mate10 \n LGVU6 \n Iphone11"
print(producer)
print(product)
#What if I wanted to start the months on a new line? You simply start the string with \n, like this: "\nJan\nFeb\nMar\nApr\nMay\nJun\nJul\nAug"
