myname = "Thuan"
thuan_age = "35" #not a lie
thuan_height = 75 #inches
thuan_weight = 150 #lbs
thuan_eyes = "Brown"
thuan_teeth = "White" #smoking
thuan_hair = "Black"
print(f"Let's talk about {myname}.")
print(f"Thuan's {thuan_age} years old")
print(f"Thuan is {thuan_height} pounds heavy")
print(f"Thuan: {thuan_age}, {thuan_height}, {thuan_weight}, {thuan_eyes}, {thuan_teeth}, {thuan_hair}")

